{
    "root": true,
    "parser": "@typescript-eslint/parser",

    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": ["react"],
    "ignorePatterns": ["*.css", "*.test.tsx"],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
